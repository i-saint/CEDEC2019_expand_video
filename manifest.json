{
  "name": "CEDEC2022 expand video",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://cedec.cesa.or.jp/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["content.js"],
      "matches": ["https://cedec.cesa.or.jp/*"]
    }
  ]
}